index="policyguru_data" resourceId="enum*" ("INBOUND" OR "disposition.disposition"="ALLOWED")
| rename  "connections{}.left.facets{}.number" as sourcenumber
| rename  "disposition.disposition" as disposition
| stats values(sourcenumber) as sourcenumber
        values(destinationnumber) as destinationnumber
        values(meta) as meta
        values(disposition) as disposition
        by guid
| search disposition = "ALLOWED"
| search meta="*INBOUND*"
| lookup pg_directory_listings.csv number AS sourcenumber OUTPUT lists
| rename lists as sourcelists
| search NOT sourcelists="*Whitelist*"
| search NOT sourcelists="*WhiteList*"
| search NOT sourcelists="*Graylist*"
| stats count
 
